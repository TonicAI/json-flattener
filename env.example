# Fabricate API Configuration
# Copy this file to .env and fill in your actual values

# Your Fabricate API key (required)
FABRICATE_API_KEY=sk-your-api-key-here

# Fabricate URI Base (defaults to production if not set)
# FABRICATE_URI_BASE=http://localhost:3000

# Workspace name (required)
WORKSPACE=your-workspace-name

# Database name (required)  
DATABASE=your-database-name

# Entity/table name (optional, can be provided via command line)
ENTITY=users
